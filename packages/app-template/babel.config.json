{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "useESModules": false,
                "regenerator": false
            }
        ]
    ],
    "env": {
        "test": {
            "presets": [
                [
                    "@babel/preset-env",
                    {
                        "targets": "current node"
                    }
                ]
            ]
        },
        "node": {
            "plugins": ["dynamic-import-node"]
        }
    }
}
