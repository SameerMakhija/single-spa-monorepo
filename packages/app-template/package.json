        "@testing-library/jest-dom": "^5.11.8",
        "@testing-library/react": "^11.2.2",
        "@types/jest": "^26.0.19",
        "@types/systemjs": "^6.1.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^26.6.3",
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.3",
        "eslint": "^7.17.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-jsx-a11y": "^6.4.1",
